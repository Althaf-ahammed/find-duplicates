def find_duplicates(list_of_numbers):
    lis=[]
    double=[]
    for item in list_of_numbers:
        if (item not in lis):
            lis.append(item)
        elif(item not in double):
            double.append(item)
        
    return double

list_of_numbers=[1,2,3,4]
list_of_duplicates=find_duplicates(list_of_numbers)
print(list_of_duplicates)
